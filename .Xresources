xterm*faceName: Ubuntu Mono Regular:antialias=true
xterm*font: 7x13
xterm*faceSize: 10
xterm*selectToClipboard: true
XTerm*foreground: rgb:b2/b2/b2
XTerm*background: rgb:08/08/08
Xterm*saveLines: 30000

!URxvt*font: 7x13
!URxvt*faceSize: 11
!URxvt.font: xft:Source Code Pro:size=11
!URxvt.font: xft:DejaVu Sans Mono:size=10
URxvt.font: xft:SourceCodePro:size=10,xft:monaco:size=10,xft:FontAwesome:size=10,xft:Monospace:size=10
!URxvt.font: xft:SauceCodePro:size=10,xft:monaco:size=10,xft:FontAwesome:size=10,xft:Monospace:size=10

URxvt.scrollBar_right: False
URxvt.scrollTtyOutput: False
URxvt.scrollTtyKeypress: True
URxvt.letterSpace: -1
URxvt.saveLines: 65535
URxvt.scrollBar: False
URxvt*foreground: rgb:b2/b2/b2
!URxvt*foreground: rgb:08/08/08
URxvt*background: rgb:08/08/08
!URxvt*background: rgb:ff/ff/ee
URxvt.color12: rgb:5c/5c/ff
URxvt.color4: rgb:88/88/ff
URxvt.imLocale: en_IE.utf8
URxvt.perl-ext-common:      default,matcher,clipboard,keyboard-select,resize-font
! url-select disabled
!URxvt.url-select.launcher:  firefox
!URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up

! Base16 Default Dark
! Scheme: Chris Kempson (http://chriskempson.com)

!#define base00 #181818
!#define base01 #282828
!#define base02 #383838
!#define base03 #585858
!#define base04 #b8b8b8
!#define base05 #d8d8d8
!#define base06 #e8e8e8
!#define base07 #f8f8f8
!#define base08 #ab4642
!#define base09 #dc9656
!#define base0A #f7ca88
!#define base0B #a1b56c
!#define base0C #86c1b9
!#define base0D #7cafc2
!#define base0E #ba8baf
!#define base0F #a16946
!
!*foreground:   base05
!#ifdef background_opacity
!*background:   [background_opacity]base00
!#else
!*background:   base00
!#endif
!*cursorColor:  base05
!
!*color0:       base00
!*color1:       base08
!*color2:       base0B
!*color3:       base0A
!*color4:       base0D
!*color5:       base0E
!*color6:       base0C
!*color7:       base05
!
!*color8:       base03
!*color9:       base08
!*color10:      base0B
!*color11:      base0A
!*color12:      base0D
!*color13:      base0E
!*color14:      base0C
!*color15:      base07
!
!! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
!! use 'shell' template to set these if necessary
!*color16:      base09
!*color17:      base0F
!*color18:      base01
!*color19:      base02
!*color20:      base04
!Xft.autohint: 0
!
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb*color21:      base06
